<odoo>
    <data noupdate="0">

        <template id="pendaftaran_online_web" inherit_id="website.assets_frontend" name="Website assets">
            <xpath expr="." position="inside">
                <script type="text/javascript" src="/aa_pendaftaran_online/static/src/js/pendaftaran.js"></script>
            </xpath>
        </template>

        <record id="website_menu_online_reg" model="website.menu">
            <field name="name">Pendaftaran User</field>
            <field name="url">/pendaftaran/form</field>
            <field name="parent_id" ref="website.main_menu"/>
            <field name="sequence" type="int">80</field>
        </record>

    </data>
</odoo>
